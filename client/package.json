{
  "name": "sample-items-scoring-guide-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "less": "lessc-each styles/ dist/css/",
    "less:dev": "lessc-each SmarterBalancedStyles/styles dist/css",
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@osu-cass/react-advanced-filter": "^1.0.5",
    "@osu-cass/smarter-balanced-styles": "^1.0.8",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.3",
    "@types/enzyme-adapter-react-16": "^1.0.0",
    "@types/jest": "^20.0.8",
    "@types/jquery": "3.2.12",
    "@types/react": "^16.0.21",
    "@types/react-dom": "^16.0.2",
    "awesome-typescript-loader": "^3.3.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "font-awesome": "^4.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jquery": "3.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "less-loader": "^4.0.5",
    "lessc-each": "1.1.0",
    "react-addons-test-utils": "^15.6.2",
    "source-map-loader": "0.2.1",
    "style-loader": "^0.19.0",
    "ts-jest": "20.0.7",
    "typescript": "^2.5.3",
    "url-loader": "^0.5.9",
    "webpack": "3.5.5",
    "webpack-dev-server": "^2.9.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx?)$",
    "collectCoverageFrom": [
      "src/**/*.tsx",
      "!src/**/__tests__/*",
      "!src/index.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.json"
      },
      "babelConfig": {
        "presets": [
          "es2015",
          "env",
          "react"
        ],
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "identity-obj-proxy",
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js"
    }
  }
}
