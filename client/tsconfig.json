{
  "compilerOptions": {
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "types": [
      "jest"
    ]
  }
}
