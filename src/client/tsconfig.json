{
  "compilerOptions": {
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noEmitOnError": false,
    "removeComments": false,
    "sourceMap": true,
    "module": "es2015",
    "target": "es5",
    "outDir": "../../dist/public/js",
    "jsx": "react",
    "strictNullChecks": true,
    "lib": ["es2015", "dom"]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "node_modules/@types/node"
  ],
  "compileOnSave": true
}
